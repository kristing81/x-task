<div class="container">
  <div class="row">
    <div class="col-md-10">
      <h1>All Teams</h1>
        <% @teams.each do |team| %>
          <p><%= link_to team.name, team_path(team.id) %></p>
        <% end %>
        <%= link_to "Add a Team", new_team_path, class: "btn btn-info" %>
    </div>
  </div>
</div>
